<div class="feeds-container">
  <div class="feed-container" id="recently-created-feed">
  	<h3>Recently Created</h3>
    <% @recently_created.each do |p| %>
    	<% if !p.videos.empty? then %>
      	<div class="feed-playlist" onclick="redirect('/watch?playlist=<%= p.id %>')"><%= p.name %></div>
      <% end %>
    <% end %>
  </div>
</div>