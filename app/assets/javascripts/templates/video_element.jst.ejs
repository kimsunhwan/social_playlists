<div class="video-cell-template">
	<div class="video-info-container">
		<div class="video-name"><%= name %></div>
	</div>
	<div class="thumb-container" onmouseover="highlightThumbnail(<%= id %>)" onmouseout="unhighlightThumbnail(<%= id %>)">
		<img id="thumbnail<%= id %>" src="http://img.youtube.com/vi/<%= site_code %>/default.jpg" alt="<%= name %>" />
	</div>
</div>